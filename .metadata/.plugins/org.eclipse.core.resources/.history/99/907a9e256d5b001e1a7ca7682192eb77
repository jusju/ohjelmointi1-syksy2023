		System.out.println("Luvut lopuksi: ");		
		for (int i = 0; i < taulukko.length; i++) {
			System.out.println(taulukko[i]);
		}
		
	}
	
	
	public void vaihdaKeskenaan(int[] luvut) {
		
	}
	
